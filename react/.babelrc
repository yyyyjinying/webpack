{
    "presets": [
        "@babel/preset-react", 
        [
            "@babel/preset-env", // 配置名
            { // options
                "loose": false, // 低版本的浏览器开启true
                "modules": false
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators",{ "legacy": true }], // 配置支持装饰器
        [
            "import", {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": true
            }
        ]
      
    ],
    "env": {
        "development": {
            "presets": [
                [
                    "@babel/preset-react", 
                    { "development": true }
                ]
            ]
        }
    }
}